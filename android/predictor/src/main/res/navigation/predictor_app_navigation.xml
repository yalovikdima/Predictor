<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:id="@+id/predictor_app_navigation"
    app:startDestination="@id/predictionFragment">

    <fragment
        android:id="@+id/predictionFragment"
        android:name="com.origins.predictor.features.prediction.ui.PredictionFragment"
        android:label="PredictorFragment" >
        <action
            android:id="@+id/action_predictorFragment_to_validateDialogFragment"
            app:destination="@id/validateDialogFragment" />
    </fragment>

    <dialog
        android:id="@+id/validateDialogFragment"
        android:name="com.origins.predictor.features.dialog.validate.ValidateDialogFragment"
        android:label="ValidateDialogFragment" />
</navigation>